<div class="container">
  <h1>注文履歴一覧</h1>
  
  <table class="table">
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
    <% @cart_items.each do |cart_item| %>
      <tr>
        <td><%= link_to cart_item.created_at, admin_order_path(cart_item.customer.id) %></td>
        <td><%= cart_item.customer.full_name %></td>
        <td><%= cart_item.amount %></td>
        <td><%= %></td>
      </tr>
    <% end %>
  </table>
</div>